    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Papdl Slice",
            "type": "python",
            "request":"launch",
            "cwd": "${workspaceFolder}",
            "module":"papdl",
            "args": ["slice", "${workspaceRoot}/scripts/sample_ff_model"]
        }
    ]
}